{
  "name": "ts-skeleton",
  "version": "1.0.0",
  "description": "An empty TS skeleton fully configured with ESLint, Prettier, Husky and Vitest",
  "main": "src/index.ts",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "tsc",
    "prepare": "husky install"
  },
  "keywords": [
    "typescript",
    "eslint",
    "prettier",
    "vitest"
  ],
  "author": "Pedro Lopez Mareque",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.2",
    "express-prom-bundle": "^6.4.1",
    "pidusage": "^3.0.0",
    "prom-client": "^14.0.1",
    "typescript": "^4.5.5",
    "vite": "^2.7.13"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/pidusage": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.2.2",
    "prettier": "^2.5.1",
    "vitest": "^0.1.27"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  }
}
